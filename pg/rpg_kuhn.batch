#!/bin/bash
#SBATCH --partition cpufast
#SBATCH --nodes 1
#SBATCH --ntasks-per-node 1
#SBATCH --mem-per-cpu 8G
#SBATCH --time 0-4:00:00
#SBATCH --job-name rpg_kuhn
#SBATCH --output slogs/rpg_kuhn-%J.log

module purge
singularity exec ~/openspiel.sif python3 pg/policy_gradient.py \
    --game kuhn_poker \
    --logname rpg_kuhn_128_128 \
    --loss_str rpg \
    --num_episodes 1000000 \
    --hidden_layers_sizes=128,128
