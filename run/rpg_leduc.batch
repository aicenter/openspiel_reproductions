#!/bin/bash
#SBATCH --partition cpu
#SBATCH --nodes 1
#SBATCH --ntasks-per-node 1
#SBATCH --mem-per-cpu 8G
#SBATCH --time 1-0:00:00
#SBATCH --job-name rpg_leduc
#SBATCH --output slogs/rpg_leduc-%J.log

module purge
singularity exec ~/openspiel.sif python3 pg/policy_gradient.py \
    --game leduc_poker \
    --logname rpg_leduc_16m \
    --loss_str rpg \
    --num_episodes 16000000 \
    --hidden_layers_sizes=128,128
