#!/bin/bash
#SBATCH --partition cpufast
#SBATCH --nodes 1
#SBATCH --ntasks-per-node 1
#SBATCH --mem-per-cpu 8G
#SBATCH --time 0-4:00:00
#SBATCH --job-name ed_kuhn
#SBATCH --output slogs/ed_kuhn-%J.log

module purge
singularity exec ~/openspiel.sif python3 exp_descent/nn_exp_descent.py \
    --game_name kuhn_poker \
    --logname ed_kuhn_nn_adam
    --init_lr 1
    --lr_decay .999
    --num_layers 2
    --num_hidden 256
    --regularizer_scale .001